pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                echo 'this is a checkout stage'
            }
        }
        
        stage('Build') {
            steps {
                echo 'this a build stage'
            }
        }
        
         stage('Test') {
            steps {
                echo 'this a test stagedd'
            }
        }
    }
}
